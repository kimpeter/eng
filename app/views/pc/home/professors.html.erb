<script>
  $(function(){
    $('.js-remindMe').on('click', function(){
      var link = $(this);
      var url = link.data('post-url');
      var course_id = link.data('course-id');
      var course_title = link.data('course-title');
      var submit = $('.modal').find('input[type=submit]');
      $('.js-remindMe-courseTitle').html(course_title);
      submit.prop('disabled', true);
      $('.modal').on('keyup', '#email', function(e){
        if (this.checkValidity()){
          submit.prop('disabled', false);
        } else {
          submit.prop('disabled', true);
        }
      })
      $('.modal').on('submit', function(e){
        e.preventDefault();
  
        $.post(url, $(this).serialize(), function(notification, status, jqXHR){
          if (!notification.error){
            link.replaceWith('<span class="label"><i class="icn icn--correct"></i> We\'ll Remind You</span>');
            $.cookie("course_reminder_" + course_id, 'true');
            $('.modal-close').click();
          } else {
            $('.modal').find('.help').text(notification.error);
            $.rails.enableElement(submit);
            submit.val(submit.text());
            submit.prop('disabled', false);
          };
  
        }, 'json');
      });
    });
  
    $('.js-remindMe').each(function(elem){
      var link = $(this);
      var course_id = link.data('course-id');
      if($.cookie('course_reminder_' + course_id) === 'true'){
        link.replaceWith('<span class="label"><i class="icn icn--check"></i> We\'ll Remind You</span>');
      };
    });
  });
</script>
